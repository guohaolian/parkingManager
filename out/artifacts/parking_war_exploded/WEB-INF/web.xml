<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginHandle</servlet-name>
        <servlet-class>ServletHandle.LoginHandle</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginHandle</servlet-name>
        <url-pattern>/LoginHandle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RoleHandle</servlet-name>
        <servlet-class>ServletHandle.RoleHandle</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RoleHandle</servlet-name>
        <url-pattern>/RoleHandle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserHandle</servlet-name>
        <servlet-class>ServletHandle.UserHandle</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserHandle</servlet-name>
        <url-pattern>/UserHandle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CardHandle</servlet-name>
        <servlet-class>ServletHandle.CardHandle</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CardHandle</servlet-name>
        <url-pattern>/CardHandle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FixedHandle</servlet-name>
        <servlet-class>ServletHandle.FixedHandle</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FixedHandle</servlet-name>
        <url-pattern>/FixedHandle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TempHandle</servlet-name>
        <servlet-class>ServletHandle.TempHandle</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TempHandle</servlet-name>
        <url-pattern>/TempHandle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SeatHandle</servlet-name>
        <servlet-class>ServletHandle.SeatHandle</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SeatHandle</servlet-name>
        <url-pattern>/SeatHandle</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>Login.jsp</welcome-file>
    </welcome-file-list>

    <error-page>
        <error-code>500</error-code>
        <location>/Admin/_Error.jsp</location>
    </error-page>
</web-app>